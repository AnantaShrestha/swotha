        function calculateGrandtotal() {
            var sabaipaisa = [];
            all = $('#grandtotal > tbody > tr');
            var count = 0;
            all.each(function () {
                if (count < 4) {
                    var dollar = ($('td:eq(3)', this).text());
                    var str = dollar.replace(/[^0-9.]/g, "") + '';
                    var s = Number(str.split(',').join(''));
