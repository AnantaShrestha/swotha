                animateOut: "slideOutDown",
                animateIn: "flipInX",
                autoplay: !0,
                autoplayTimeout: 4e3,
                autoplayHoverPause: !0,
                responsive: {
                    0: {items: 1, nav: !0},
                    600: {items: 1, nav: !0},
                    768: {items: 1, nav: !0},
                    1e3: {items: 1, nav: !0}
                }
            })
        })
